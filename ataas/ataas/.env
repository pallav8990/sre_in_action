# Core
ATAAS_BASE_URL=https://ataas.example.com
SREHUB_ATAAS_PROJECT=platform-prod

# Auth strategy (used by auth_from_env("ATAAS"))
ATAAS_AUTH_TYPE=api_key
ATAAS_API_KEY=your-super-secret-key
ATAAS_API_KEY_HEADER=x-api-key

# Optional discovery filters
SREHUB_ATAAS_ALLOW=db-*,image-*,k8s-*
SREHUB_ATAAS_DENY=exp-*
SREHUB_ATAAS_REQUIRE_TAGS=stable,ga